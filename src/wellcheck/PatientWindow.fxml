<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import wellcheck.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml" fx:controller="wellcheck.PatientWindowController">
  <children>
    <TabPane layoutY="45.0" prefHeight="355.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
      <tabs>
        <Tab fx:id="patientstab2" text="Patients">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <TableView fx:id="patientTable2" prefHeight="319.0" prefWidth="384.0">
                  <columns>
                    <TableColumn prefWidth="192.0" text="Patient" fx:id="patient2">
                      <cellValueFactory>
                        <PropertyValueFactory property="patient" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="192.0" text="Doctor" fx:id="doctor2">
                      <cellValueFactory>
                        <PropertyValueFactory property="doctor" />
                      </cellValueFactory>
                    </TableColumn>
                  </columns>
                   <items>
                     <FXCollections fx:id ="patientList2" fx:factory="observableArrayList">
                         
                     </FXCollections>
                   </items>
                </TableView>
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="datatab2" text="Data">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="310.0" prefWidth="286.0">
              <children>
                <TableView layoutY="26.0" prefHeight="298.0" prefWidth="600.0">
                  <columns>
                    <TableColumn prefWidth="75.0" text="BP">
                      <cellValueFactory>
                        <PropertyValueFactory property="Blood" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="75.0" text="Weight">
                      <cellValueFactory>
                        <PropertyValueFactory property="weight" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="75.0" text="Sugar">
                      <cellValueFactory>
                        <PropertyValueFactory property="sugar" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="105.0" text="Date">
                      <cellValueFactory>
                        <PropertyValueFactory property="date" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="275.0" text="Comments">
                      <cellValueFactory>
                        <PropertyValueFactory property="comments" />
                      </cellValueFactory>
                    </TableColumn>
                  </columns>
                  <items>
                     <FXCollections fx:id ="dataList2" fx:factory="observableArrayList">
                         
                     </FXCollections>
                   </items>
                </TableView>
                <ComboBox fx:id="patientDropDown2" layoutY="3.0" prefWidth="143.0" onAction="#dataComboBox2">
                    <items>
                    <FXCollections fx:id="comboList2" fx:factory="observableArrayList">
                    </FXCollections>
                  </items>
            </ComboBox>
                <Button fx:id="graphButton2" layoutX="150.0" layoutY="3.0" mnemonicParsing="false" text="Graph" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="prescriptionstab2" text="Prescriptions">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <TableView prefHeight="319.0" prefWidth="384.0">
                  <columns>
                    <TableColumn prefWidth="192.0" text="Patient" />
                    <TableColumn prefWidth="192.0" text="Doctor" />
                  </columns>
                </TableView>
              </children>
            </AnchorPane>
          </content>
        </Tab>
      </tabs>
    </TabPane>
    <Hyperlink id="logOut2" layoutX="537.0" layoutY="9.0" text="Log Out" textFill="#1400ff" underline="true" onAction="#logOut2" />
    <Label fx:id="typeLabel2" layoutX="14.0" layoutY="14.0" />
  </children>
</AnchorPane>
