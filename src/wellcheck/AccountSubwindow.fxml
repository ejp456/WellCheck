            <AnchorPane fx:id="informationpane" prefHeight="174.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <Button fx:id="saveinformationbutton" layoutX="14.0" layoutY="139.0" mnemonicParsing="false" onAction="#saveinformation" text="Save Information" />
