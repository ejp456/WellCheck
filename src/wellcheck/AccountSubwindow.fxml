            </AnchorPane>
            <AnchorPane fx:id="questionpane" prefHeight="174.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                <Button fx:id="changequestionbutton" layoutX="14.0" layoutY="139.0" mnemonicParsing="false" onAction="#changequestion" text="Change Question" />
                <TextArea id="challengequestion" fx:id="challengequestionfield" layoutX="12.0" layoutY="30.0" prefHeight="62.0" prefWidth="222.0" wrapText="true" />
