                <TextField fx:id="lnamefield" layoutX="124.0" layoutY="43.0" prefWidth="108.0" />
                <TextField fx:id="datemfield" layoutX="124.0" layoutY="72.0" prefWidth="27.0" promptText="MM" />
                <TextField fx:id="datedfield" layoutX="151.0" layoutY="72.0" prefWidth="27.0" promptText="DD" />
                <TextField fx:id="dateyfield" layoutX="176.0" layoutY="72.0" prefWidth="56.0" promptText="YYYY" />
