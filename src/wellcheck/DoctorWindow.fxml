                        <PropertyValueFactory property="patient" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="192.0" text="Doctor" fx:id="doctor">
                      <cellValueFactory>
                        <PropertyValueFactory property="doctor" />
                      </cellValueFactory>
                    </TableColumn>
                  </columns>
                                    <items>
                                        <FXCollections fx:id ="patientList" fx:factory="observableArrayList">
                         
                                        </FXCollections>
                                    </items>
                                </TableView>
