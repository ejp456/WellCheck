<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import wellcheck.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml" fx:controller="wellcheck.DoctorWindowController">
  <children>
    <TabPane layoutY="45.0" prefHeight="355.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
      <tabs>
        <Tab fx:id="patientstab" text="Patients">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <TableView fx:id="patientTable" prefHeight="319.0" prefWidth="384.0">
                  <columns>
                    <TableColumn prefWidth="192.0" text="Patient" fx:id="patient">
                      <cellValueFactory>
                        <PropertyValueFactory property="patient" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="192.0" text="Doctor" fx:id="doctor">
                      <cellValueFactory>
                        <PropertyValueFactory property="doctor" />
                      </cellValueFactory>
                    </TableColumn>
                  </columns>
                   <items>
                     <FXCollections fx:id ="patientList" fx:factory="observableArrayList">
                         
                     </FXCollections>
                   </items>
                </TableView>
                <Button fx:id="removePatient" layoutX="417.0" layoutY="284.0" mnemonicParsing="false" onAction="#removePatient" prefWidth="183.0" text="Remove Patient" />
                <Button fx:id="addDependant" layoutX="417.0" layoutY="254.0" mnemonicParsing="false" onAction="#addDependant" prefWidth="183.0" text="Add Dependant to Patient" />
                <Button fx:id="editPatient" layoutX="417.0" layoutY="226.0" mnemonicParsing="false" onAction="#editPatient" prefWidth="183.0" text="Edit Patient Information" />
                <Button fx:id="addPatient" layoutX="417.0" layoutY="197.0" mnemonicParsing="false" onAction="#addPatientButton" prefWidth="183.0" text="Add Patient" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="datatab" text="Data">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="310.0" prefWidth="286.0">
              <children>
                <TableView layoutY="26.0" prefHeight="298.0" prefWidth="600.0">
                  <columns>
                    <TableColumn prefWidth="75.0" text="BP">
                      <cellValueFactory>
                        <PropertyValueFactory property="Blood" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="75.0" text="Weight">
                      <cellValueFactory>
                        <PropertyValueFactory property="weight" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="75.0" text="Sugar">
                      <cellValueFactory>
                        <PropertyValueFactory property="sugar" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="105.0" text="Date">
                      <cellValueFactory>
                        <PropertyValueFactory property="date" />
                      </cellValueFactory>
                    </TableColumn>
                    <TableColumn prefWidth="275.0" text="Comments">
                      <cellValueFactory>
                        <PropertyValueFactory property="comments" />
                      </cellValueFactory>
                    </TableColumn>
                  </columns>
                  <items>
                     <FXCollections fx:id ="dataList" fx:factory="observableArrayList">
                         
                     </FXCollections>
                   </items>
                </TableView>
                 <ComboBox fx:id="patientDropDown" layoutY="3.0" onAction="#dataComboBox" prefWidth="143.0">
                    <items>
                    <FXCollections fx:id="comboList" fx:factory="observableArrayList">
                    </FXCollections>
                  </items>
            </ComboBox>
                <Button fx:id="graphButton" layoutX="150.0" layoutY="3.0" mnemonicParsing="false" onAction="#showGraph" text="Graph" />
                <Button id="graphButton" fx:id="addEntry" layoutX="211.0" layoutY="3.0" mnemonicParsing="false" onAction="#newEntry" text="Add" />
                <Button id="graphButton" fx:id="editEntry" layoutX="259.0" layoutY="3.0" mnemonicParsing="false" onAction="#newEntry" text="Edit" />
              </children>
            </AnchorPane>
          </content>
        </Tab>
        <Tab fx:id="prescriptionstab" text="Prescriptions">
          <content>
            <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
              <children>
                <TableView prefHeight="319.0" prefWidth="384.0">
                  <columns>
                    <TableColumn prefWidth="192.0" text="Patient" />
                    <TableColumn prefWidth="192.0" text="Doctor" />
                  </columns>
                </TableView>
              </children>
            </AnchorPane>
          </content>
        </Tab>
      </tabs>
    </TabPane>
    <Hyperlink id="logOut" layoutX="537.0" layoutY="9.0" onAction="#logOut" text="Log Out" textFill="#1400ff" underline="true" />
  </children>
</AnchorPane>
